<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <include layout="@layout/layout_usercenter_contacts_topbar"/>
    <ViewStub
        android:id="@+id/vs_userCenter_contacts_friendReuquest"
        android:layout="@layout/layout_usercenter_contacts_friendrequest_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <com.flyco.tablayout.CommonTabLayout
        android:id="@+id/tl_userCenter_contacts_type"
        android:background="#33999B"
        app:tl_textSelectColor="@android:color/white"
        app:tl_textUnselectColor="@android:color/darker_gray"
        android:layout_width="match_parent"
        android:layout_height="50dp"/>
    <FrameLayout
        android:id="@+id/placeHolder_userCenter_contacts_type"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</LinearLayout>